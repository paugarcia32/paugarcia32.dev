{
  "name": "astro-nano",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently --names \"ASTRO,WATCH\" --prefix-colors \"cyan,magenta\" \"astro dev\" \"tsx scripts/watch-frontmatter.ts\"",
    "dev:no-sync": "astro dev",
    "dev:network": "concurrently --names \"ASTRO,WATCH\" --prefix-colors \"cyan,magenta\" \"astro dev --host\" \"tsx scripts/watch-frontmatter.ts\"",
    "build": "pnpm sync-frontmatter && astro check && astro build",
    "preview": "astro preview",
    "preview:network": "astro preview --host",
    "astro": "astro",
    "lint": "biome lint .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "sync-frontmatter": "tsx scripts/sync-frontmatter.ts",
    "create-content": "tsx scripts/create-content.ts"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.0.2",
    "@astrojs/rss": "^4.0.10",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/tailwind": "^5.1.3",
    "@fontsource/inter": "^5.0.17",
    "@fontsource/lora": "^5.0.16",
    "@tailwindcss/typography": "^0.5.10",
    "@vercel/analytics": "^1.6.1",
    "astro": "^5.0.5",
    "astro-mermaid": "^1.3.1",
    "clsx": "^2.1.0",
    "fuse.js": "^7.1.0",
    "mermaid": "^11.12.2",
    "sharp": "^0.33.3",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2"
  },
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67",
  "devDependencies": {
    "@biomejs/biome": "2.3.15",
    "@inquirer/prompts": "^8.2.1",
    "@types/node": "^25.2.3",
    "chokidar": "^5.0.0",
    "concurrently": "^9.2.1",
    "glob": "^13.0.3",
    "tsx": "^4.21.0"
  }
}
